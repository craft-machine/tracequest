{
  "manifest_version": 2,
  "name": "Tracequest",
  "description": "Inspect requests made by your backend.",
  "version": "0.0.1",
  "version_name": "0.0.1",
  "minimum_chrome_version": "49",
  "devtools_page": "main.html",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "main.html"
  ],
  "permissions": [
    "file:///*",
    "http://*/*",
    "https://*/*"
  ]
}